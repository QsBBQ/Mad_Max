<h2>Subnet <%= subnet.network %><h2>
  <section>
  <table id="subnet_table">
    <% subnet.ip_addresses.each do |ip| %>
      <tr>
        <th>IP Address</th>
        <th>DNS Name</th>
        <th>Status</th>
        <th>Comments</th>
      </tr>
      <tr>
        <td><%= ip.address %></td>
        <td><%= ip.dns_name %></td>
        <td><%= ip.status %></td>
        <td><%= ip.comments %></td>
      </tr>
    <% end %>
  </table>
  </section>
